package day0621;

public class QuizFor_09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//Q.1 2 4 5 7 8 10 _continue 이용해서 출력해보세요
		
		int i=1;
		
		for(;i<=10;i++)
		{
			if(i%3==0)
				continue;
			System.out.print(i+" ");
		
		}
		System.out.println();
		
	}

}
