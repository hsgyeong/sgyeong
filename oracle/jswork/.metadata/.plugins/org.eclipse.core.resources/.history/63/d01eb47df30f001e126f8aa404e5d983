package day0621;

public class ExForWhile_10 {

	public static void test1() {
		
		//for문으로 1부터 5까지 합 구하기(반복문 안의 num을 sum에 더하는 문제) 
		
		int sum=0;
		for(int i=1;i<=5;i++)
		{
			sum=sum+i;
		}
			System.out.println("총합계는 "+sum);
	}	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	}
	
	

}
