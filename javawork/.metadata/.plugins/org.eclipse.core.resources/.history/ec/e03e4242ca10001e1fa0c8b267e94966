package day0622;

public class ForStar_07 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//중첩(다중)for문으로 별모양 찍기
		System.out.println("중첩(다중)for문으로 별모양 찍기");
		
	
				for(int k=1;k<=3;k++)
				{
				System.out.print("*");
		
				System.out.println();
						
				}
					
				
			
			
			
	


	




