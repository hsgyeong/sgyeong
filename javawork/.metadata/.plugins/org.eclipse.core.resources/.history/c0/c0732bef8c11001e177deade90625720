package day0623;

import java.util.Scanner;

public class ArrScannerSum_09 {

	public static void main(String[] args) {
		/*
		 0번지 값: 55
		 1번지 값: 77
		 2번지 값: 88
		 3번지 값: 49
		 4번지 값: 29
		 =================
		 [입력값 확인]
		 1번째값: 55
		 2번째값: 77
		 3번째값: 88
		 4번째값: 49
		 5번째값: 29
		 =================
		 총합계: 298
		 	
		 */
		
		
		int [] data;
		data=new int [5];
		
		Scanner sc=new Scanner(System.in);
		
		int sum=0;
		int su;
		
		for(int i=0;i<=4;i++)
		{
			System.out.println("[입력값 확인]");
			System.out.println(i+"번지 값: "+su);
		}	
		
	}

}
