package day0628;


class Score{
	
	/*
	 중간고사 시험결과
	 학생명: 김지윤
	 자바점수: 99
	 오라클점수: 88
	 HTML점수: 56
	 ==================
	  학생명: 김지윤
	 자바점수: 99
	 오라클점수: 88
	 HTML점수: 56
	 */
	
	
	
	private String stuName;
	private int java;
	private int oracle;
	private int html;
	
	

	//자동 setter, getter
		
	static final String TITLE="중간고사 시험결과";

	public String getStuName() {
		return stuName;
	}

	public void setStuName(String stuName) {
		this.stuName = stuName;
	}

	public int getJava() {
		return java;
	}

	public void setJava(int java) {
		this.java = java;
	}

	public int getOracle() {
		return oracle;
	}

	public void setOracle(int oracle) {
		this.oracle = oracle;
	}

	public int getHtml() {
		return html;
	}

	public void setHtml(int html) {
		this.html = html;
	}
	
		
}

