package day0629;


class MyTv{
	
	/* **tv시청목록**

	ebs 채널을 시청합니다
	볼륨은 15데시벨 입니다
	---------------
	tvn 채널을 시청합니다
	볼륨은 22데시벨 입니다
	*/
	
	private String che; //채널
	private String vol; //볼륨
	
	
	public String getChe() {
		return che;
	}
	public void setChe(String che) {
		this.che = che;
	}
	public String getVol() {
		return vol;
	}
	public void setVol(String vol) {
		this.vol = vol;
	}
	
	
	public String getChannel()
	{
		
	}
		
}
